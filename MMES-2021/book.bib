}

@article{Gorman2018b,
  title = {The Myth of the Secular\textendash{{Islamist}} Divide in {{Muslim}} Politics: {{Evidence}} from {{Tunisia}}},
  volume = {61},
  abstract = {The paper presents a Computational Fluid Dynamics (CFD) simulation of the distribution of influx gas in the drilling fluid during kick situations. Special focus is put on the bottom-hole inlet region of a vertical well, with the aim of obtaining a highly resolved phase distribution. An interfacial tracking method (VOF) is used to simulate the unsteady motion of incompressible drilling fluid and compressible gas, taking into account turbulence as well as non-Newtonian rheology. This method describes both phases with a single set of equations and additionally solves an equation to calculate the local volume fraction of the phases. Hence no a priori assumptions need to be made regarding flow patterns. The result of these simulations will subsequently be used to model the chemical reaction kinetics of the buffering process between H2S influxes and high-pH drilling fluid environments.},
  number = {1},
  journal = {Current Sociology},
  author = {Gorman, Brandon},
  year = {2018},
  keywords = {Cognitive interviewing,ideology,Islamism,mixed methods,polarization,secularism,Tunisia},
  pages = {145--164}
}

@article{Adcock2001,
  title = {Measurement {{Validity}}: {{A Shared Standard}} for {{Qualitative}} and {{Quantitative Research}}},
  volume = {95},
  abstract = {Scholars routinely make claims that presuppose the validity of the\$\textbackslash{}backslash\$nobservations and measurements that operationalize their concepts.\$\textbackslash{}backslash\$nYet, despite recent advances in political science methods, surprisingly\$\textbackslash{}backslash\$nlittle attention has been devoted to measurement validity. We address\$\textbackslash{}backslash\$nthis gap by exploring four themes. First, we seek to establish a\$\textbackslash{}backslash\$nshared framework that allows quantitative and qualitative scholars\$\textbackslash{}backslash\$nto assess more effectively, and communicate about, issues of valid\$\textbackslash{}backslash\$nmeasurement. Second, we underscore the need to draw a clear distinction\$\textbackslash{}backslash\$nbetween measurement issues and disputes about concepts. Third, we\$\textbackslash{}backslash\$ndiscuss the contextual specificity of measurement claims, exploring\$\textbackslash{}backslash\$na variety of measurement strategies that seek to combine generality\$\textbackslash{}backslash\$nand validity by devoting greater attention to context. Fourth, we\$\textbackslash{}backslash\$naddress the proliferation of terms for alternative measurement validation\$\textbackslash{}backslash\$nprocedures and offer an account of the three main types of validation\$\textbackslash{}backslash\$nmost relevant to political scientists.},
  number = {3},
  journal = {American Political Science Review},
  author = {Adcock, Robert and Collier, David},
  year = {2001},
  pages = {529--546}
}

@article{Brooke2017a,
  title = {Sectarianism and Social Conformity: {{Evidence}} from {{Egypt}}},
  volume = {70},
  issn = {10659129},
  abstract = {Why might citizens adopt exaggerated public antagonism toward outgroups? When this is so, how much do public and private attitudes diverge? I argue that expanding exclusionary rhetoric against outgroups can create social pressures that incentivize ordinary citizens to adopt bigoted attitudes to avoid ostracism from their own majority community. Based on an investigation of Egypt during the Arab Spring, I identify the emergence and diffusion of a norm of discrimination against the country's tiny Shi'a population. Under these conditions, a substantial portion of Sunni citizens adopted and countenanced anti-Shi'a bigotry not because they truly believed it, but rather because they feared the consequences of expressing public support for coexistence. A variety of qualitative evidence traces the growth of anti-Shi'a sentiment during this period, while original survey data show that over 80 percent of Sunni respondents openly expressed anti-Shi'a attitudes. Yet when asked about their attitudes via an item count technique, a method that grants a reprieve from social pressures, the percentage of respondents expressing discriminatory views toward the Shi'a dropped to just over 40 percent. One implication is that sectarian attitudes in the region are as much the product of malleable social and political pressures as deeply rooted preferences.},
  number = {4},
  journal = {Political Research Quarterly},
  doi = {10.1177/1065912917717641},
  author = {Brooke, Steven},
  year = {2017},
  keywords = {Egypt,Sectarianism,Survey experiment},
  pages = {848--860}
}

@article{Fair2018a,
  title = {Conceptions of {{Shari}}'a and {{Support}} for {{Militancy}} and {{Democratic Values}}: {{Evidence}} from {{Pakistan}}},
  volume = {6},
  abstract = {Numerous empirical studies of the relationship between popular support for Islamism and support for democracy and violence have yielded inconclusive results. We suspect that this is largely because scholars have not operationalized support for Shari`a in ways that capture the concept's multi-dimensionality. In this paper, we employ data derived from a carefully designed survey instrument that casts unique insights into how Pakistanis imagine a Shari`a -based government. We find that formalizing an Islamic government as one that implements Shari`a by providing services and security for its citizens is positively associated with support for democratic values, whereas conceptualizing it as one that implements Shari`a by imposing hudud punishments and restricting women's public roles is positively associated with support for militancy. These results suggest that it is important to understand how individuals within a particular context construe a Shari`a -based government, and future empirical work should take this.},
  number = {3},
  journal = {Political Science Research and Methods},
  author = {Fair, C. Christine and Littman, Rebecca and Nugent, Elizabeth R.},
  year = {2018},
  pages = {429--448}
}


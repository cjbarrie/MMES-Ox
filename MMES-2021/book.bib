@book{Teorell2010,
  address = {{Cambridge}},
  title = {Determinants of Democratization: {{Explaining}} Regime Change in the {{World}}, 1972\textendash{}2006},
  abstract = {Why do some countries democratize whereas others do not? Taht is the \$3.6 billion question -at least to the U.S. govenment, which spent that amount in 2010 with the stated aim of advancing democracy, human rights, and good governance around the world (ForeignAssistnace.gov). Despite de significant and growing investment by the United States and other Western governments in democracy promotion, authoritarianism is though to be on the rise. In its annual report on democracy in the orld, Freedom House (2011) stated that global freedom "worsened for the fifth consecutive year in 2010", which was the longest period of declines since the organization's start in 1972 (p.2). Thus, Jan Teorell's new book makes a timely contribution to the literature on the auses of democratization.},

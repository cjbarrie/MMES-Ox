@incollection{Thompson2004,
  address = {{London}},
  title = {Democratic Revolutions: {{Asia}} and {{Eastern Europe}}},
  abstract = {``People power'' in the Philippines, the fall of the Berlin Wall, and the toppling of the Milo{\v s}evi\textonequarter{} regime in Serbia are examples of how important democratic revolutions have become in the modern world, yet these have neither been added to the revolutionary canon nor adequately incorporated into the literature on democratic transitions. In theorizing peaceful popular uprisings against dictator- ships and comparing them across Asia and Eastern Europe, Democratic Revolutions revives a much-neglected concept. Despite enormous differences between Asia and Eastern Europe, there are striking similarities between the non-violent, spontaneous, urban-based and cross- class mass movements that overthrew unyielding rulers and led to the establishment of democracy in the two regions. The book explores the kind of non-democratic regimes that are particularly vulnerable to democratic revolutions. It examines why and how democrats rebel and what the results of democratic revolutions have been. Questions posed in this book include: \textbullet{} Why communist rulers shot in China but not in Eastern Europe; \textbullet{} Why stolen elections led to the overthrow of Milo{\v s}evi\textonequarter{} in Serbia; \textbullet{} Why there have been so many women leading democratic revolutions in Asia; \textbullet{} Why democratic revolutions have been able to overcome a supposed ``clash of civilizations.'' This book attempts to democratize theories of revolution and revolutionize democratic transitions. Cases and comparisons are drawn from fifteen democratic revolutions over the last two decades and the book includes in-depth studies of East Germany, China, Serbia and the Philippines.},
  booktitle = {Democratic Revolutions: {{Asia}} and Eastern Europe},
  publisher = {{Routledge}},
  author = {Thompson, Mark R.},
  year = {2004},
  pages = {1-180}
}


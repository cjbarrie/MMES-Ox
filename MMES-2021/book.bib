@article{Rule1972,
  title = {1830 and the {{Unnatural History}} of {{Revolution}}},
  volume = {28},
  issn = {0022-4537, 0022-4537},
  abstract = {When natural histories of revolution do not consist simply of explicated definitions, they generally fail as explanatory theories, largely because they fail to deal with negative cases or to eliminate alternative explanations, \& because they assume that a continuous \& integrated soc system experiences revolution. A more adequate treatment requires that gov's, contenders for power, \& relations among them be distinguished. Revolution consists of the fragmentation of a single polity. Starting from that view, a pol'al process model of revolutionary change is developed, illustrated \& supported by detailed evidence\textendash{}both quantitative \& qualitative\textendash{}from the French Revolution of 1830. It is felt that the evidence presented raises doubts about conventional natural-historical analysis, but it is insufficient to rule out a number of alternative interpretations of revolutionary processes\textendash{}including the more sophisticated frustration-aggression formulations which have been appearing in recent yrs. However the search among the intellectual avenues now open to the student of revolution finally turns out, it will surely take us to a much more explicit theory of the pol'al process itself than modern sociol'ts \& psychol'ts of revolution have been willing to employ. Modified HA.},
  number = {1},

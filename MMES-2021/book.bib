@article{Smith1981,
  title = {War and Ethnicity: {{The}} Role of Warfare in the Formation, Self-Images and Cohesion of Ethnic Communities},
  volume = {4},
  abstract = {The article studies the relationship between war and ethnicity. Although there is vast literature on both, scholars have devoted little systematic attention to their interrelation. Even in the recently expanding field of the effects of war on society, the factor of ethnicity is hardly mentioned. This serious omission stems not only from the inherent difficulty of generalizing about the vast historical and sociological field, but also from the tacit prevalence of a particular view of the interrelation, a view that is only now receiving a partial scrutiny. Here the prevalent view which sees warfare as largely the product of society and of its out-group hostilities is challenged, and instead the view that war has been a powerful factor in shaping certain crucial aspects of ethnic community and nationhood is argued. The influence of war has, however, been uneven; it has affected some aspects of ethnic community more than others. Moreover, that influence has been both direct and mediated; and in some ways, the indirect influences have been both more interesting and pervasive.},
  number = {4},
  journal = {Ethnic and Racial Studies},
  author = {Smith, Anthony D.},
  year = {1981},
  pages = {375-397}
}

